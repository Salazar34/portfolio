{
  "name": "sal_code_portfolio",
  "version": "1.0.0",
  "description": "Sal Code personal portfolio",
  "main": "index.js",
  "scripts": {
    "test": "npm run format && npm run lint && npm run compile",
    "format": "npx prettier --write assets/**/*.{js,css,scss}",
    "lint": "npx eslint",
    "compile": "npx sass --style=compressed assets/scss/main.scss:assets/css/style.css",
    "watch": "npx sass --watch assets/scss/main.scss:assets/css/style.css"
  },
  "keywords": [
    "portfolio",
    "html",
    "css",
    "javascript",
    "staticwebsite",
    "salcode",
    "html5",
    "css3",
    "animations",
    "prettier",
    "eslint"
  ],
  "author": "Sal Code",
  "repository": {
    "type": "github",
    "url": "https://github.com/Salazar34/portfolio"
  },
  "readme": "README.md",
  "license": "MIT",
  "dependencies": {
    "animate.css": "^4.1.1",
    "bootstrap-icons": "^1.7.2",
    "sass": "^1.45.1"
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1"
  }
}
